local gravityEnabled = true
local gravityValue = workspace.Gravity

Tab:Section({Title = "üåå –ì—Ä–∞–≤–∏—Ç–∞—Ü–∏—è"})

Tab:Toggle({
    Title = "–í–∫–ª—é—á–∏—Ç—å/–í—ã–∫–ª—é—á–∏—Ç—å –≥—Ä–∞–≤–∏—Ç–∞—Ü–∏—é",
    Desc = "–ü–æ–∑–≤–æ–ª—è–µ—Ç –≤–∫–ª—é—á–∞—Ç—å/–≤—ã–∫–ª—é—á–∞—Ç—å –≥—Ä–∞–≤–∏—Ç–∞—Ü–∏—é",
    Value = true,
    Callback = function(v)
        gravityEnabled = v
        if gravityEnabled then
            workspace.Gravity = gravityValue
        else
            workspace.Gravity = 0
        end
        togglesound()
    end
})

Tab:Slider({
    Title = "–ó–Ω–∞—á–µ–Ω–∏–µ –≥—Ä–∞–≤–∏—Ç–∞—Ü–∏–∏",
    Desc = "–£—Å—Ç–∞–Ω–æ–≤–∏—Ç–µ –∑–Ω–∞—á–µ–Ω–∏–µ –≥—Ä–∞–≤–∏—Ç–∞—Ü–∏–∏",
    Min = 0,
    Max = 500,
    Rounding = 0,
    Value = workspace.Gravity,
    Callback = function(val)
        gravityValue = val
        if gravityEnabled then
            workspace.Gravity = gravityValue
        end
        slidersound()
    end
})

