-- –§—É–Ω–∫—Ü–∏—è –ø–æ–∏—Å–∫–∞ –∏–≥—Ä—ã –ø–æ ID
local function findGameByPlaceId(placeId)
    for gameKey, gameData in pairs(GameDatabase) do
        for _, gamePlaceId in ipairs(gameData.placeIds) do
            if gamePlaceId == placeId then
                return gameData
            end
        end
    end
    return nil
end

-- –§—É–Ω–∫—Ü–∏—è –±–µ–∑–æ–ø–∞—Å–Ω–æ–π –∑–∞–≥—Ä—É–∑–∫–∏ —Å–∫—Ä–∏–ø—Ç–∞
local function loadScriptSafe(url)
    Window:Notify({
        Title = "‚è≥ –ó–∞–≥—Ä—É–∑–∫–∞...",
        Desc = "–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –ø–æ–¥–æ–∂–¥–∏—Ç–µ",
        Time = 2
    })

    local success, result = pcall(function()
        local scriptContent = game:HttpGet(url, true)
        if scriptContent and scriptContent ~= "404: Not Found" then
            local loadedFunction = loadstring(scriptContent)
            if loadedFunction then
                return loadedFunction()
            else
                error("–û—à–∏–±–∫–∞ –∫–æ–º–ø–∏–ª—è—Ü–∏–∏ —Å–∫—Ä–∏–ø—Ç–∞")
            end
        else
            error("–°–∫—Ä–∏–ø—Ç –Ω–µ –Ω–∞–π–¥–µ–Ω")
        end
    end)

    if success then
        Window:Notify({
            Title = "‚úÖ –£—Å–ø–µ—à–Ω–æ",
            Desc = "–°–∫—Ä–∏–ø—Ç –∑–∞–≥—Ä—É–∂–µ–Ω –∏ –≤—ã–ø–æ–ª–Ω–µ–Ω!",
            Time = 3
        })
        notifysound()
    else
        local errorMsg = tostring(result):sub(1, 100)
        Window:Notify({
            Title = "‚ùå –û—à–∏–±–∫–∞",
            Desc = errorMsg,
            Time = 5
        })
        warn("–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ —Å–∫—Ä–∏–ø—Ç–∞:", result)
    end
end

-- –í–ö–õ–ê–î–ö–ê MAIN
local Tab = Window:Tab({Title = "üéÆ Main", Icon = "tree-pine"})

Tab:Section({Title = "üë§ –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–µ–º"})

local walkSpeed = 16
local jumpPower = 50

Tab:Slider({
    Title = "–°–∫–æ—Ä–æ—Å—Ç—å —Ö–æ–¥—å–±—ã",
    Desc = "–ò–∑–º–µ–Ω—è–µ—Ç —Å–∫–æ—Ä–æ—Å—Ç—å –ø–µ—Ä–µ–¥–≤–∏–∂–µ–Ω–∏—è",
    Min = 16,
    Max = 200,
    Rounding = 0,
    Value = 16,
    Callback = function(val)
        walkSpeed = val
        local character = LocalPlayer.Character
        if character and character:FindFirstChild("Humanoid") then
            character.Humanoid.WalkSpeed = val
        end
        slidersound()
    end
})

Tab:Slider({
    Title = "–°–∏–ª–∞ –ø—Ä—ã–∂–∫–∞",
    Desc = "–ò–∑–º–µ–Ω—è–µ—Ç –≤—ã—Å–æ—Ç—É –ø—Ä—ã–∂–∫–∞",
    Min = 50,
    Max = 500,
    Rounding = 0,
    Value = 50,
    Callback = function(val)
        jumpPower = val
        local character = LocalPlayer.Character
        if character and character:FindFirstChild("Humanoid") then
            character.Humanoid.JumpPower = val
        end
        slidersound()
    end
})

Tab:Toggle({
    Title = "‚ö° –ê–≤—Ç–æ-–ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ",
    Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø—Ä–∏–º–µ–Ω—è—Ç—å –Ω–∞—Å—Ç—Ä–æ–π–∫–∏",
    Value = true,
    Callback = function(v)
        if v then
            RunService.Heartbeat:Connect(function()
                local character = LocalPlayer.Character
                if character and character:FindFirstChild("Humanoid") then
                    character.Humanoid.WalkSpeed = walkSpeed
                    character.Humanoid.JumpPower = jumpPower
                end
            end)
        end
        togglesound()
    end
})

Tab:Button({
    Title = "üîÑ –°–±—Ä–æ—Å–∏—Ç—å",
    Desc = "–°–±—Ä–æ—Å–∏—Ç—å –≤—Å–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞",
