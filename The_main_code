        humanoid.WalkSpeed = 150
        humanoid.JumpPower = 200
        print("–°—É–ø–µ—Ä —Å–∫–æ—Ä–æ—Å—Ç—å –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞!")
    end
