local defaultScript = [[-- üéÑ Saul Hub Executor
print("bye from Saul Hub v3.4!")

-- –ü—Ä–∏–º–µ—Ä: –ë—ã—Å—Ç—Ä—ã–π —Å—Ç–∞—Ä—Ç
local player = game.Players.LocalPlayer

if player.Character then
    local humanoid = player.Character:FindFirstChild("Humanoid")
    if humanoid then
        humanoid.WalkSpeed = 50
        humanoid.JumpPower = 100
        print("–°–∫–æ—Ä–æ—Å—Ç—å —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞!")
    end
end

-- –° –ù–æ–≤—ã–º –ì–æ–¥–æ–º! üéÖ
]]

local currentScript = defaultScript

local codeDisplay = Tab6:Code({
    Title = "üìù –¢–µ–∫—É—â–∏–π —Å–∫—Ä–∏–ø—Ç",
    Code = currentScript
})

local scriptInfo = Tab6:Label({
    Title = "üìä –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è",
    Desc = "–°–∏–º–≤–æ–ª–æ–≤: " .. #currentScript .. " | –°—Ç—Ä–æ–∫: " .. select(2, string.gsub(currentScript, "\n", "")) + 1
})

local function updateScriptInfo()
    local charCount = #currentScript
    local lineCount = select(2, string.gsub(currentScript, "\n", "")) + 1
    scriptInfo:SetDesc(string.format("–°–∏–º–≤–æ–ª–æ–≤: %d | –°—Ç—Ä–æ–∫: %d", charCount, lineCount))
end

local scriptInput = Tab6:Textbox({
    Title = "‚úèÔ∏è –ë—ã—Å—Ç—Ä–æ–µ —Ä–µ–¥–∞–∫—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ",
    Desc = "–í–≤–µ–¥–∏—Ç–µ –Ω–æ–≤—ã–π —Å–∫—Ä–∏–ø—Ç",
    Placeholder = "print('–ù–æ–≤—ã–π –≥–æ–¥ –Ω–∞—Å—Ç—É–ø–∏–ª! üéÑ')",
    Value = "",
    ClearText = false,
    Callback = function(text)
        if text and text ~= "" then
            currentScript = text
            codeDisplay:SetCode(text)
            updateScriptInfo()
        end
    end
})

Tab6:Section({Title = "‚öôÔ∏è –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ"})

Tab6:Button({
    Title = "üöÄ –í—ã–ø–æ–ª–Ω–∏—Ç—å",
    Desc = "–ó–∞–ø—É—Å—Ç–∏—Ç—å —Ç–µ–∫—É—â–∏–π —Å–∫—Ä–∏–ø—Ç",
    Callback = function()
        btnclick()

        if not currentScript or currentScript:gsub("%s", "") == "" then
            Window:Notify({
                Title = "‚ö†Ô∏è –í–Ω–∏–º–∞–Ω–∏–µ",
                Desc = "–°–∫—Ä–∏–ø—Ç –ø—É—Å—Ç–æ–π!",
                Time = 3
            })
            return
        end

        Window:Notify({
            Title = "‚ö° –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ...",
            Desc = "–°–∫—Ä–∏–ø—Ç –∑–∞–ø—É—Å–∫–∞–µ—Ç—Å—è",
            Time = 2
        })

        local success, result = pcall(function()
            local fn = loadstring(currentScript)
            if fn then
                return fn()
            else
                error("–û—à–∏–±–∫–∞ –∫–æ–º–ø–∏–ª—è—Ü–∏–∏")
            end
        end)

        if success then
            Window:Notify({
                Title = "‚úÖ –£—Å–ø–µ—à–Ω–æ",
                Desc = "–°–∫—Ä–∏–ø—Ç –≤—ã–ø–æ–ª–Ω–µ–Ω",
                Time = 3
            })
        else
            local errorMsg = tostring(result):sub(1, 100)
            Window:Notify({
                Title = "‚ùå –û—à–∏–±–∫–∞",
                Desc = errorMsg,
                Time = 5
            })
        end
    end
})

Tab6:Button({
    Title = "üîÑ –ü—Ä–∏–º–µ—Ä",
    Desc = "–ó–∞–≥—Ä—É–∑–∏—Ç—å –ø—Ä–∏–º–µ—Ä —Å–∫—Ä–∏–ø—Ç–∞",
    Callback = function()
        btnclick()
        currentScript = defaultScript
        codeDisplay:SetCode(defaultScript)
        scriptInput:SetValue("")
        updateScriptInfo()
    end
})

