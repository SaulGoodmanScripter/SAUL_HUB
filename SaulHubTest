local defaultScript = [[-- Saul Hub Executor
print("Hello from Saul Hub!")

-- Example script
local player = game.Players.LocalPlayer

if player.Character then
    local humanoid = player.Character:FindFirstChild("Humanoid")
    if humanoid then
        humanoid.WalkSpeed = 50
        humanoid.JumpPower = 100
        print("Speed set!")
    end
end]]

local currentScript = defaultScript

local codeDisplay = Tab5:Code({
    Title = "Current Script",
    Code = currentScript
})

local scriptInfo = Tab5:Label({
    Title = "Information",
    Desc = "Characters: " .. #currentScript .. " | Lines: " .. select(2, string.gsub(currentScript, "\n", "")) + 1
})

local function updateScriptInfo()
    local charCount = #currentScript
    local lineCount = select(2, string.gsub(currentScript, "\n", "")) + 1
    scriptInfo:SetDesc(string.format("Characters: %d | Lines: %d", charCount, lineCount))
end

local scriptInput = Tab5:Textbox({
    Title = "Edit Script",
    Desc = "Enter your Lua code",
    Placeholder = "print('Hello from Saul Hub!')",
    Value = "",
    ClearText = false,
    Callback = function(text)
        if text and text ~= "" then
            currentScript = text
            codeDisplay:SetCode(text)
            updateScriptInfo()
        end
    end
})

Tab5:Section({Title = "Script Management"})

Tab5:Button({
    Title = "Execute",
    Desc = "Run current script",
    Callback = function()
        btnclick()

        if not currentScript or currentScript:gsub("%s", "") == "" then
            Window:Notify({
                Title = "Warning",
                Desc = "Script is empty!",
                Time = 3
            })
            return
        end

        Window:Notify({
            Title = "Executing...",
            Desc = "Script is running",
            Time = 2
        })

        local success, result = pcall(function()
            local fn = loadstring(currentScript)
            if fn then
                return fn()
            else
                error("Compilation error")
            end
        end)

        if success then
            Window:Notify({
                Title = "Success",
                Desc = "Script executed",
                Time = 3
            })
        else
            local errorMsg = tostring(result):sub(1, 100)
            Window:Notify({
                Title = "Error",
                Desc = "Error: " .. errorMsg,
                Time = 5
            })
        end
    end
})

Tab5:Button({
    Title = "Paste from Clipboard",
    Desc = "Paste script from clipboard",
    Callback = function()
        btnclick()
        pcall(function()
            local clipboard = game:HttpGet("http://127.0.0.1:6463/rpc?v=1", true)
            if clipboard then
                currentScript = clipboard
                codeDisplay:SetCode(clipboard)
                updateScriptInfo()
                Window:Notify({
                    Title = "Pasted",
                    Desc = "Script loaded from clipboard",
                    Time = 3
                })
            end
        end)
    end
})

