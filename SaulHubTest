Tab6:Button({
    Title = "üéØ –ü—Ä–∏–º–µ—Ä—ã",
    Desc = "–ë—ã—Å—Ç—Ä—ã–µ –ø—Ä–∏–º–µ—Ä—ã —Å–∫—Ä–∏–ø—Ç–æ–≤",
    Callback = function()
        btnclick()
        
        local examples = {
            "fly = false\nspeed = 1\nfunction startFly()\n    repeat wait()\n        if fly and game.Players.LocalPlayer.Character then\n            local char = game.Players.LocalPlayer.Character\n            char.Humanoid.PlatformStand = true\n            local bv = Instance.new('BodyVelocity')\n            bv.Parent = char.HumanoidRootPart\n            bv.Velocity = Vector3.new()\n        end\n    until not fly\nend",
            "for _, v in pairs(game.Players:GetPlayers()) do\n    if v ~= game.Players.LocalPlayer then\n        print('–ò–≥—Ä–æ–∫: ' .. v.Name)\n    end\nend",
            "local light = Instance.new('PointLight')\nlight.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart\nlight.Brightness = 5\nlight.Range = 20\nlight.Color = Color3.fromRGB(255, 0, 0)"
        }
        
        currentScript = examples[math.random(1, #examples)]
        codeDisplay:SetCode(currentScript)
        updateScriptInfo()
        
        Window:Notify({
            Title = "üéØ –ü—Ä–∏–º–µ—Ä –∑–∞–≥—Ä—É–∂–µ–Ω",
            Desc = "–ì–æ—Ç–æ–≤ –∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—é!",
            Time = 3
        })
    end
})

-- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ —Ç–µ–∫—É—â–µ–º —ç–∫—Å–ø–ª–æ–∏—Ç–µ
Tab6:Section({Title = "‚ÑπÔ∏è –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è"})

Tab6:Label({
    Title = "üé≠ –ü–∞—Ä–æ–¥–∏–∏ –Ω–∞ —ç–∫—Å–ø–ª–æ–∏—Ç—ã",
    Desc = "–≠—Ç–∏ —Å–∫—Ä–∏–ø—Ç—ã —Å–æ–∑–¥–∞—é—Ç –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å—ã,\n–ø–æ—Ö–æ–∂–∏–µ –Ω–∞ –ø–æ–ø—É–ª—è—Ä–Ω—ã–µ —ç–∫—Å–ø–ª–æ–∏—Ç—ã\n(—Ç–æ–ª—å–∫–æ UI, –Ω–µ —Ñ—É–Ω–∫—Ü–∏–æ–Ω–∞–ª)"
})

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ, –µ—Å–ª–∏ –µ—Å—Ç—å —ç–∫—Å–ø–ª–æ–∏—Ç
spawn(function()
    wait(2)
    local exploitDetected = false
    local exploitName = ""
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –Ω–∞–ª–∏—á–∏–µ –∏–∑–≤–µ—Å—Ç–Ω—ã—Ö —ç–∫—Å–ø–ª–æ–∏—Ç–æ–≤
    if syn and syn.request then
        exploitDetected = true
        exploitName = "Synapse X"
    elseif KRNL_LOADED then
        exploitDetected = true
        exploitName = "Krnl"
    elseif identifyexecutor and identifyexecutor() then
        exploitDetected = true
        exploitName = identifyexecutor()
    end
    
    if exploitDetected then
        Window:Notify({
            Title = "üîç –û–±–Ω–∞—Ä—É–∂–µ–Ω —ç–∫—Å–ø–ª–æ–∏—Ç",
            Desc = "–ò—Å–ø–æ–ª—å–∑—É–µ—Ç—Å—è: " .. exploitName,
            Time = 5
        })
    end
end)

-- –í–ö–õ–ê–î–ö–ê AUTO SCRIPTS
local Tab7 = Window:Tab({Title = "üéØ Auto-scripts", Icon = "bell"})

local currentGameId = game.PlaceId
local currentGameData = findGameByPlaceId(currentGameId)

if currentGameData then
    Tab7:Section({Title = "üéÆ –û–±–Ω–∞—Ä—É–∂–µ–Ω–∞ –∏–≥—Ä–∞: " .. currentGameData.name})

    Tab7:Label({
        Title = "üü¢ –ò–≥—Ä–∞ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç—Å—è",
        Desc = "–î–æ—Å—Ç—É–ø–Ω–æ " .. #currentGameData.scripts .. " —Å–∫—Ä–∏–ø—Ç–æ–≤"
    })

    for _, scriptData in ipairs(currentGameData.scripts) do
        Tab7:Code({
            Title = "üìÑ " .. scriptData.title,
            Code = "loadstring(game:HttpGet('" .. scriptData.url .. "'))()"
        })

        Tab7:Button({
            Title = "üöÄ " .. scriptData.title,
            Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π –∑–∞–ø—É—Å–∫",
            Callback = function()
                btnclick()
                loadScriptSafe(scriptData.url)
            end
        })
    end
else
    Tab7:Section({Title = "‚ùå –ò–≥—Ä–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω–∞"})

    local gameName = "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –∏–≥—Ä–∞"
    pcall(function()
        gameName = MarketplaceService:GetProductInfo(currentGameId).Name
    end)

    Tab7:Label({
        Title = "‚ÑπÔ∏è –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± –∏–≥—Ä–µ",
        Desc = gameName .. "\nID: " .. currentGameId
    })
end
-- –í–ö–õ–ê–î–ö–ê LOW POP SERVERS (–≤—Å—Ç—Ä–æ–µ–Ω–Ω–∞—è –≤ UI)
local TabServers = Window:Tab({Title = "üåê Low Pop", Icon = "users"})

TabServers:Section({Title = "üìä –ü–æ–∏—Å–∫ –º–∞–ª–æ–ª—é–¥–Ω—ã—Ö —Å–µ—Ä–≤–µ—Ä–æ–≤"})

-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ
local serverList = {}
local isLoading = false
local maxServers = 20

-- –°—Ç–∞—Ç—É—Å–Ω–∞—è –º–µ—Ç–∫–∞
local statusLabel = TabServers:Label({
    Title = "‚ÑπÔ∏è –ì–æ—Ç–æ–≤ –∫ –ø–æ–∏—Å–∫—É",
    Desc = "–ù–∞–∂–º–∏—Ç–µ '–ù–∞–π—Ç–∏ —Å–µ—Ä–≤–µ—Ä–∞' –¥–ª—è –Ω–∞—á–∞–ª–∞"
})

-- –§—É–Ω–∫—Ü–∏—è –ø–æ–ª—É—á–µ–Ω–∏—è —Å–µ—Ä–≤–µ—Ä–æ–≤
local function fetchServers(sortOrder)
    local success, result = pcall(function()
        local url = string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=%s&limit=100", 
            game.PlaceId, sortOrder or "Asc")
        
        local response = game:HttpGet(url, true)
        return game:GetService("HttpService"):JSONDecode(response)
    end)
    
    return success and result or nil
end

-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –∫–Ω–æ–ø–æ–∫ –¥–ª—è —Å–µ—Ä–≤–µ—Ä–æ–≤
local function createServerButtons()
    -- –û—á–∏—â–∞–µ–º —Å–µ–∫—Ü–∏—é (–∫—Ä–æ–º–µ —Å—Ç–∞—Ç–∏—á–µ—Å–∫–∏—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤)
    -- –í —Ä–µ–∞–ª—å–Ω–æ–π —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏ –∑–¥–µ—Å—å –Ω—É–∂–Ω–æ —É–¥–∞–ª–∏—Ç—å –¥–∏–Ω–∞–º–∏—á–µ—Å–∫–∏–µ —ç–ª–µ–º–µ–Ω—Ç—ã
    
    if #serverList == 0 then
        TabServers:Label({
            Title = "üì≠ –ù–µ—Ç –¥–∞–Ω–Ω—ã—Ö",
            Desc = "–ù–∞–∂–º–∏—Ç–µ '–ù–∞–π—Ç–∏ —Å–µ—Ä–≤–µ—Ä–∞'"
        })
        return
    end
    
    local displayed = 0
    for _, server in ipairs(serverList) do
        if displayed >= maxServers then break end
        
        local playerCount = server.playing
        local maxPlayers = server.maxPlayers
        local fillPercentage = math.floor((playerCount / maxPlayers) * 100)
        
        -- –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Å—Ç–∞—Ç—É—Å —Å–µ—Ä–≤–µ—Ä–∞
        local status = ""
        local statusColor = ""
        if playerCount == 0 then
            status = "üü¢ –ü–£–°–¢–û–ô"
            statusColor = Color3.fromRGB(0, 255, 0)
        elseif playerCount <= 3 then
            status = "üü° –ú–ê–õ–û"
            statusColor = Color3.fromRGB(255, 255, 0)
        elseif playerCount <= 10 then
            status = "üü† –°–†–ï–î–ù–ï"
            statusColor = Color3.fromRGB(255, 165, 0)
        else
            status = "üî¥ –ú–ù–û–ì–û"
            statusColor = Color3.fromRGB(255, 0, 0)
        end
        
        -- –°–æ–∑–¥–∞–µ–º –∫–Ω–æ–ø–∫—É –¥–ª—è —Å–µ—Ä–≤–µ—Ä–∞
        TabServers:Button({
            Title = string.format("üë• %d/%d | %s", playerCount, maxPlayers, status),
            Desc = string.format("–ó–∞–ø–æ–ª–Ω–µ–Ω–Ω–æ—Å—Ç—å: %d%% | ID: %s", fillPercentage, server.id or "Unknown"),
            Callback = function()
                btnclick()
                
                if server.id then
                    Window:Notify({
                        Title = "üöÄ –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è...",
                        Desc = string.format("–ù–∞ —Å–µ—Ä–≤–µ—Ä —Å %d –∏–≥—Ä–æ–∫–∞–º–∏", playerCount),
                        Time = 3
                    })
                    
                    -- –ü—ã—Ç–∞–µ–º—Å—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞—Ç—å—Å—è
                    pcall(function()
                        game:GetService("TeleportService"):TeleportToPlaceInstance(
                            game.PlaceId,
                            server.id,
                            LocalPlayer
                        )
                    end)
                else
                    Window:Notify({
                        Title = "‚ùå –û—à–∏–±–∫–∞",
                        Desc = "ID —Å–µ—Ä–≤–µ—Ä–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω",
                        Time = 3
                    })
                end
            end
        })
        
        displayed = displayed + 1
    end
end

-- –ö–Ω–æ–ø–∫–∏ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
TabServers:Section({Title = "‚öôÔ∏è –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ"})

TabServers:Button({
    Title = "üîç –ù–∞–π—Ç–∏ —Å–µ—Ä–≤–µ—Ä–∞ (–ú–µ–Ω—å—à–µ –∏–≥—Ä–æ–∫–æ–≤)",
    Desc = "–°–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –æ—Ç –ø—É—Å—Ç—ã—Ö –∫ –∑–∞–ø–æ–ª–Ω–µ–Ω–Ω—ã–º",
    Callback = function()
        btnclick()
        
        if isLoading then return end
        isLoading = true
        
        statusLabel:SetTitle("‚è≥ –ü–æ–∏—Å–∫...")
        statusLabel:SetDesc("–ó–∞–ø—Ä–∞—à–∏–≤–∞–µ–º —Å–ø–∏—Å–æ–∫ —Å–µ—Ä–≤–µ—Ä–æ–≤")
        
        spawn(function()
            local data = fetchServers("Asc") -- Asc = –æ—Ç –º–µ–Ω—å—à–µ–≥–æ –∫ –±–æ–ª—å—à–µ–º—É
            
            if data and data.data then
                serverList = data.data
                
                -- –§–∏–ª—å—Ç—Ä—É–µ–º —Ç–æ–ª—å–∫–æ —Å–µ—Ä–≤–µ—Ä–∞ —Å –∏–≥—Ä–æ–∫–∞–º–∏ –º–µ–Ω—å—à–µ –º–∞–∫—Å
                local filtered = {}
                for _, server in ipairs(serverList) do
                    if server.playing < server.maxPlayers then
                        table.insert(filtered, server)
                    end
                end
                serverList = filtered
                
                statusLabel:SetTitle(string.format("‚úÖ –ù–∞–π–¥–µ–Ω–æ: %d", #serverList))
                statusLabel:SetDesc(string.format("–°–µ—Ä–≤–µ—Ä–æ–≤ —Å < %d –∏–≥—Ä–æ–∫–∞–º–∏", maxServers))
                
                createServerButtons()
                
                Window:Notify({
                    Title = "‚úÖ –°–µ—Ä–≤–µ—Ä–∞ –Ω–∞–π–¥–µ–Ω—ã",
                    Desc = string.format("–ù–∞–π–¥–µ–Ω–æ %d –¥–æ—Å—Ç—É–ø–Ω—ã—Ö —Å–µ—Ä–≤–µ—Ä–æ–≤", #serverList),
                    Time = 3
                })
            else
                statusLabel:SetTitle("‚ùå –û—à–∏–±–∫–∞")
                statusLabel:SetDesc("–ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å —Å–ø–∏—Å–æ–∫ —Å–µ—Ä–≤–µ—Ä–æ–≤")
                
                Window:Notify({
                    Title = "‚ùå –û—à–∏–±–∫–∞",
                    Desc = "–ü—Ä–æ–≤–µ—Ä—å—Ç–µ –∏–Ω—Ç–µ—Ä–Ω–µ—Ç —Å–æ–µ–¥–∏–Ω–µ–Ω–∏–µ",
                    Time = 5
                })
            end
            
            isLoading = false
        end)
    end
})

TabServers:Button({
    Title = "üìä –û–±–Ω–æ–≤–∏—Ç—å —Å–ø–∏—Å–æ–∫",
    Desc = "–û–±–Ω–æ–≤–∏—Ç—å —Ç–µ–∫—É—â–∏–π —Å–ø–∏—Å–æ–∫ —Å–µ—Ä–≤–µ—Ä–æ–≤",
    Callback = function()
        btnclick()
        if #serverList > 0 then
            createServerButtons()
            Window:Notify({
                Title = "üîÑ –û–±–Ω–æ–≤–ª–µ–Ω–æ",
                Desc = "–°–ø–∏—Å–æ–∫ —Å–µ—Ä–≤–µ—Ä–æ–≤ –æ–±–Ω–æ–≤–ª–µ–Ω",
                Time = 2
            })
        end
    end
})

TabServers:Button({
    Title = "üéØ –ë—ã—Å—Ç—Ä—ã–π —Ä–µ–¥–∂–æ–π–Ω",
    Desc = "–ú–≥–Ω–æ–≤–µ–Ω–Ω–æ –ø–µ—Ä–µ–∑–∞–π—Ç–∏ –≤ –∏–≥—Ä—É",
    Callback = function()
        btnclick()
        game:GetService("TeleportService"):Teleport(game.PlaceId, LocalPlayer)
    end
})

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏
TabServers:Section({Title = "‚öôÔ∏è –ù–∞—Å—Ç—Ä–æ–π–∫–∏"})

TabServers:Slider({
    Title = "–ú–∞–∫—Å. —Å–µ—Ä–≤–µ—Ä–æ–≤",
    Desc = "–°–∫–æ–ª—å–∫–æ —Å–µ—Ä–≤–µ—Ä–æ–≤ –ø–æ–∫–∞–∑—ã–≤–∞—Ç—å",
    Min = 5,
    Max = 50,
    Rounding = 0,
    Value = 20,
    Callback = function(val)
        maxServers = val
        slidersound()
        
        if #serverList > 0 then
            createServerButtons()
        end
    end
})

TabServers:Toggle({
    Title = "üîÑ –ê–≤—Ç–æ-–ø–æ–∏—Å–∫",
    Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –∏—Å–∫–∞—Ç—å —Å–µ—Ä–≤–µ—Ä–∞ –ø—Ä–∏ –æ—Ç–∫—Ä—ã—Ç–∏–∏",
    Value = false,
    Callback = function(v)
        autoSearch = v
        togglesound()
        
        if v and #serverList == 0 then
            -- –ò–º–∏—Ç–∏—Ä—É–µ–º –Ω–∞–∂–∞—Ç–∏–µ –∫–Ω–æ–ø–∫–∏ –ø–æ–∏—Å–∫–∞
            local searchBtn = TabServers.Buttons["üîç –ù–∞–π—Ç–∏ —Å–µ—Ä–≤–µ—Ä–∞ (–ú–µ–Ω—å—à–µ –∏–≥—Ä–æ–∫–æ–≤)"]
            if searchBtn then
                searchBtn.Callback()
            end
        end
    end
})

-- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ —Ç–µ–∫—É—â–µ–π –∏–≥—Ä–µ
TabServers:Section({Title = "üéÆ –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è"})

local gameName = "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –∏–≥—Ä–∞"
local gameId = game.PlaceId

pcall(function()
    local info = game:GetService("MarketplaceService"):GetProductInfo(gameId)
    gameName = info.Name or gameName
end)

TabServers:Label({
    Title = "–¢–µ–∫—É—â–∞—è –∏–≥—Ä–∞:",
    Desc = gameName .. "\nID: " .. gameId
})

TabServers:Label({
    Title = "üìà –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞:",
    Desc = string.format("–ò–≥—Ä–æ–∫–æ–≤ –æ–Ω–ª–∞–π–Ω: %d/%d", 
        #game:GetService("Players"):GetPlayers(),
        game:GetService("Players").MaxPlayers)
})

-- –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Å–µ—Ä–≤–µ—Ä–∞ –ø—Ä–∏ –æ—Ç–∫—Ä—ã—Ç–∏–∏ –≤–∫–ª–∞–¥–∫–∏
TabServers:Opened(function()
    if #serverList == 0 then
        -- –ï—Å–ª–∏ —Å–ø–∏—Å–æ–∫ –ø—É—Å—Ç, –ø–æ–∫–∞–∑—ã–≤–∞–µ–º –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏—é
        TabServers:Label({
            Title = "üìù –ò–Ω—Å—Ç—Ä—É–∫—Ü–∏—è:",
            Desc = "1. –ù–∞–∂–º–∏—Ç–µ '–ù–∞–π—Ç–∏ —Å–µ—Ä–≤–µ—Ä–∞'\n2. –í—ã–±–µ—Ä–∏—Ç–µ —Å–µ—Ä–≤–µ—Ä –∏–∑ —Å–ø–∏—Å–∫–∞\n3. –ù–∞–∂–º–∏—Ç–µ –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏"
        })
    end
end)

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –∏—Å–∫–∞—Ç—å —Å–µ—Ä–≤–µ—Ä–∞ –µ—Å–ª–∏ –≤–∫–ª—é—á–µ–Ω –∞–≤—Ç–æ-–ø–æ–∏—Å–∫
spawn(function()
    wait(2)
    if autoSearch and #serverList == 0 then
        local searchBtn = TabServers.Buttons["üîç –ù–∞–π—Ç–∏ —Å–µ—Ä–≤–µ—Ä–∞ (–ú–µ–Ω—å—à–µ –∏–≥—Ä–æ–∫–æ–≤)"]
        if searchBtn then
            searchBtn.Callback()
        end
    end
end)
