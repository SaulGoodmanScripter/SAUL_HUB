Tab2:Toggle({
    Title = "‚ú® –≠—Ñ—Ñ–µ–∫—Ç —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞",
    Desc = "–ü–æ–∫–∞–∑—ã–≤–∞—Ç—å –≤–∏–∑—É–∞–ª—å–Ω—ã–µ —ç—Ñ—Ñ–µ–∫—Ç—ã",
    Value = true,
    Callback = function(v)
        tpEffect = v
        togglesound()
    end
})

-- –§—É–Ω–∫—Ü–∏—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞
local function teleportTo(position)
    local char = LocalPlayer.Character
    if not char then return end
    
    local rootPart = char:FindFirstChild("HumanoidRootPart")
    if not rootPart then return end
    
    -- –î–æ–±–∞–≤–ª—è–µ–º –≤—ã—Å–æ—Ç—É
    local targetPosition = Vector3.new(
        position.X,
        position.Y + tpHeight,
        position.Z
    )
    
    -- –í–∏–∑—É–∞–ª—å–Ω—ã–µ —ç—Ñ—Ñ–µ–∫—Ç—ã
    if tpEffect then
        spawn(function()
            -- –≠—Ñ—Ñ–µ–∫—Ç –∏—Å—á–µ–∑–Ω–æ–≤–µ–Ω–∏—è
            local originalTransparency = {}
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA("BasePart") then
                    originalTransparency[part] = part.Transparency
                    part.Transparency = 0.5
                end
            end
            
            wait(0.1)
            
            -- –¢–µ–ª–µ–ø–æ—Ä—Ç
            rootPart.CFrame = CFrame.new(targetPosition)
            
            -- –≠—Ñ—Ñ–µ–∫—Ç –ø–æ—è–≤–ª–µ–Ω–∏—è
            for part, transparency in pairs(originalTransparency) do
                part.Transparency = transparency
            end
            
            -- –ß–∞—Å—Ç–∏—Ü—ã
            local particles = Instance.new("ParticleEmitter")
            particles.Texture = "rbxassetid://242563111"
            particles.Size = NumberSequence.new(1)
            particles.Transparency = NumberSequence.new(0.5)
            particles.Lifetime = NumberRange.new(1)
            particles.Rate = 50
            particles.Speed = NumberRange.new(5)
            particles.Parent = rootPart
            
            wait(0.5)
            particles:Destroy()
        end)
    else
        -- –ü—Ä–æ—Å—Ç–æ —Ç–µ–ª–µ–ø–æ—Ä—Ç
        rootPart.CFrame = CFrame.new(targetPosition)
    end
    
    -- –ó–≤—É–∫
    if tpSound then
        pcall(function()
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://911882909"
            sound.Volume = 0.3
            sound.Parent = rootPart
            sound:Play()
            game:GetService("Debris"):AddItem(sound, 2)
        end)
    end
    
    -- –£–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
    Window:Notify({
        Title = "üéØ –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω",
        Desc = string.format("X: %.1f, Y: %.1f, Z: %.1f", 
            targetPosition.X, targetPosition.Y, targetPosition.Z),
        Time = 2
    })
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –¥–ª—è –ü–ö (–º—ã—à—å)
local mouse = LocalPlayer:GetMouse()

mouse.Button1Down:Connect(function()
    if clickTPEnabled and not UserInputService.TouchEnabled then
        teleportTo(mouse.Hit.Position)
    end
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –¥–ª—è —Ç–µ–ª–µ—Ñ–æ–Ω–æ–≤ (—Ç–∞–ø)
if UserInputService.TouchEnabled then
    local touchConnections = {}
    
    Tab2:Toggle({
        Title = "üì± –¢–µ–ª–µ–ø–æ—Ä—Ç –ø–æ —Ç–∞–ø—É",
        Desc = "–¢–∞–ø –ø–æ —ç–∫—Ä–∞–Ω—É –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞",
        Value = false,
        Callback = function(v)
            if v then
                -- –°–æ–∑–¥–∞–µ–º –Ω–µ–≤–∏–¥–∏–º—É—é –∫–Ω–æ–ø–∫—É –Ω–∞ –≤–µ—Å—å —ç–∫—Ä–∞–Ω
                local touchFrame = Instance.new("Frame")
                touchFrame.Size = UDim2.new(1, 0, 1, 0)
                touchFrame.BackgroundTransparency = 1
                touchFrame.Active = true
                touchFrame.Parent = ClickTPGui
                
                local function onTouch(inputObject)
                    if clickTPEnabled and inputObject.UserInputType == Enum.UserInputType.Touch then
                        -- –ü–æ–ª—É—á–∞–µ–º –ø–æ–∑–∏—Ü–∏—é —Ç–∞–ø–∞ –≤ –º–∏—Ä–æ–≤—ã—Ö –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞—Ö
                        local camera = workspace.CurrentCamera
                        local screenPos = inputObject.Position
                        
                        -- –°–æ–∑–¥–∞–µ–º –ª—É—á –æ—Ç –∫–∞–º–µ—Ä—ã
                        local ray = camera:ViewportPointToRay(screenPos.X, screenPos.Y)
                        local raycastParams = RaycastParams.new()
                        raycastParams.FilterType = Enum.RaycastFilterType.Exclude
                        raycastParams.FilterDescendantsInstances = {LocalPlayer.Character}
                        
                        local result = workspace:Raycast(ray.Origin, ray.Direction * 1000, raycastParams)
                        
                        if result then
                            teleportTo(result.Position)
                        else
                            -- –¢–µ–ª–µ–ø–æ—Ä—Ç –≤–¥–∞–ª—å
                            teleportTo(ray.Origin + ray.Direction * 100)
                        end
                    end
                end
                
                table.insert(touchConnections, touchFrame.InputBegan:Connect(onTouch))
            else
                -- –û—Ç–∫–ª—é—á–∞–µ–º –≤—Å–µ —Å–æ–µ–¥–∏–Ω–µ–Ω–∏—è
                for _, conn in pairs(touchConnections) do
                    conn:Disconnect()
                end
                touchConnections = {}
            end
            togglesound()
        end
    })
end

-- –ì–æ—Ä—è—á–∞—è –∫–ª–∞–≤–∏—à–∞ –¥–ª—è –±—ã—Å—Ç—Ä–æ–≥–æ –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.Y then
        clickTPEnabled = not clickTPEnabled
        
        Window:Notify({
            Title = clickTPEnabled and "üéØ Click TP –í–∫–ª—é—á–µ–Ω" or "üéØ Click TP –í—ã–∫–ª—é—á–µ–Ω",
            Desc = clickTPEnabled and "–õ–ö–ú –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞" or "–ù–∞–∂–º–∏ Y —á—Ç–æ–±—ã –≤–∫–ª—é—á–∏—Ç—å",
            Time = 2
        })
    end
end)
Tab2:Toggle({
    Title = "ü§∏ Auto Jump",
    Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø—Ä—ã–≥–∞—Ç—å",
    Value = false,
    Callback = function(v)
        autoJumpEnabled = v
        
        if v then
            autoJumpConnection = RunService.Heartbeat:Connect(function()
                if autoJumpEnabled and LocalPlayer.Character then
                    local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
                    if humanoid and humanoid.FloorMaterial ~= Enum.Material.Air then
                        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                    end
                end
            end)
        else
            if autoJumpConnection then
                autoJumpConnection:Disconnect()
                autoJumpConnection = nil
            end
        end
        togglesound()
    end
})

Tab2:Slider({
    Title = "–í—ã—Å–æ—Ç–∞ –ø—Ä—ã–∂–∫–∞",
    Desc = "–í—ã—Å–æ—Ç–∞ –∞–≤—Ç–æ-–ø—Ä—ã–∂–∫–æ–≤",
    Min = 20,
    Max = 200,
    Rounding = 0,
    Value = 50,
    Callback = function(val)
        autoJumpHeight = val
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
            LocalPlayer.Character.Humanoid.JumpPower = val
        end
        slidersound()
    end
})

