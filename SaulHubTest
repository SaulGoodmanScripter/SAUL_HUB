local Tab4 = Window:Tab({Title = "‚ö° Power", Icon = "zap"})

Tab4:Section({Title = "–°–∏–ª–æ–≤—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏"})

Tab4:Toggle({
    Title = "üõ°Ô∏è God Mode",
    Desc = "–ë–µ—Å—Å–º–µ—Ä—Ç–∏–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞",
    Value = false,
    Callback = function(v)
        godMode = v
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            local humanoid = LocalPlayer.Character.Humanoid
            if v then
                humanoid.MaxHealth = math.huge
                humanoid.Health = math.huge
            else
                humanoid.MaxHealth = 100
                humanoid.Health = 100
            end
        end
        togglesound()
    end
})
-- –í Tab4 –¥–æ–±–∞–≤–∏—Ç—å —Å–µ–∫—Ü–∏—é:
Tab4:Section({Title = "üõ°Ô∏è Anti-Void Protection"})

local antiVoidEnabled = false
local voidYLevel = -10 -- —É—Ä–æ–≤–µ–Ω—å, –Ω–∏–∂–µ –∫–æ—Ç–æ—Ä–æ–≥–æ —Å—á–∏—Ç–∞–µ—Ç—Å—è "void"
local teleportPosition = Vector3.new(0, 50, 0) -- –ø–æ–∑–∏—Ü–∏—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
local checkInterval = 0.1

Tab4:Toggle({
    Title = "üõ°Ô∏è Anti-Void",
    Desc = "–ó–∞—â–∏—Ç–∞ –æ—Ç –ø–∞–¥–µ–Ω–∏—è –≤ –±–µ–∑–¥–Ω—É",
    Value = false,
    Callback = function(v)
        antiVoidEnabled = v
        togglesound()
    end
})

Tab4:Slider({
    Title = "–£—Ä–æ–≤–µ–Ω—å Void",
    Desc = "Y –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞, –Ω–∏–∂–µ –∫–æ—Ç–æ—Ä–æ–π —Ç–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ—Ç",
    Min = -100,
    Max = 50,
    Rounding = 0,
    Value = -10,
    Callback = function(val)
        voidYLevel = val
        slidersound()
    end
})

Tab4:Button({
    Title = "üìç Set Safe Position",
    Desc = "–£—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Ç–µ–∫—É—â—É—é –ø–æ–∑–∏—Ü–∏—é –∫–∞–∫ –±–µ–∑–æ–ø–∞—Å–Ω—É—é",
    Callback = function()
        btnclick()
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            teleportPosition = char.HumanoidRootPart.Position + Vector3.new(0, 5, 0)
            Window:Notify({
                Title = "üìç –ü–æ–∑–∏—Ü–∏—è —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∞",
                Desc = string.format("X:%.1f Y:%.1f Z:%.1f", 
                    teleportPosition.X, teleportPosition.Y, teleportPosition.Z),
                Time = 3
            })
        end
    end
})

-- –§—É–Ω–∫—Ü–∏—è Anti-Void
local antiVoidConnection
spawn(function()
    while wait(checkInterval) do
        if antiVoidEnabled and LocalPlayer.Character then
            local char = LocalPlayer.Character
            local rootPart = char:FindFirstChild("HumanoidRootPart")
            
            if rootPart and rootPart.Position.Y < voidYLevel then
                -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º –Ω–∞ –±–µ–∑–æ–ø–∞—Å–Ω—É—é –ø–æ–∑–∏—Ü–∏—é
                rootPart.CFrame = CFrame.new(teleportPosition)
                
                -- –í–∏–∑—É–∞–ª—å–Ω—ã–π —ç—Ñ—Ñ–µ–∫—Ç
                Window:Notify({
                    Title = "üõ°Ô∏è Anti-Void –°—Ä–∞–±–æ—Ç–∞–ª!",
                    Desc = string.format("–¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω —Å Y:%.1f", rootPart.Position.Y),
                    Time = 3
                })
                
                -- –ó–≤—É–∫–æ–≤–æ–π —ç—Ñ—Ñ–µ–∫—Ç (–µ—Å–ª–∏ –µ—Å—Ç—å)
                pcall(function()
                    local sound = Instance.new("Sound")
                    sound.SoundId = "rbxassetid://911882909"
                    sound.Volume = 0.5
                    sound.Parent = workspace
                    sound:Play()
                    wait(1)
                    sound:Destroy()
                end)
            end
        end
    end
end)
-- –í Tab4:
Tab4:Section({Title = "üì± Virtual Auto Clicker"})

local virtualClickEnabled = false
local clickPosition = Vector2.new(500, 500) -- —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞
local clickInterval = 0.5
local clickType = "Tap" -- Tap, Hold, Swipe

Tab4:Toggle({
    Title = "üì± Virtual Click",
    Desc = "–í–∏—Ä—Ç—É–∞–ª—å–Ω—ã–µ –∫–ª–∏–∫–∏ (—Ä–∞–±–æ—Ç–∞–µ—Ç –Ω–∞ —Ç–µ–ª–µ—Ñ–æ–Ω–∞—Ö)",
    Value = false,
    Callback = function(v)
        virtualClickEnabled = v
        togglesound()
    end
})

