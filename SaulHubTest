local specialScripts = {
    {"‚ö° InfiniteYield", "https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"},
    {"üíª Lua IDE", "https://raw.githubusercontent.com/RbxNoobScripter/Admin-panel-Script-/refs/heads/main/ytrewq1234556666.lua"}
