    Title = "üîÑ Dupe Item",
    Desc = "–°–∫–æ–ø–∏—Ä–æ–≤–∞—Ç—å –ø—Ä–µ–¥–º–µ—Ç –≤ —Ä—É–∫–∞—Ö (F)",
    Callback = function()
        btnclick()
        
        local player = game.Players.LocalPlayer
        local character = player.Character
        
        if not character then
            Window:Notify({
                Title = "‚ùå –û—à–∏–±–∫–∞",
                Desc = "–ü–µ—Ä—Å–æ–Ω–∞–∂ –Ω–µ –∑–∞–≥—Ä—É–∂–µ–Ω",
                Time = 3
            })
            return
        end
        
        local tool = character:FindFirstChildOfClass("Tool")
        if tool then
            local success, err = pcall(function()
                local clone = tool:Clone()
                clone.Parent = player.Backpack
                
                Window:Notify({
                    Title = "‚úÖ –£—Å–ø–µ—Ö",
                    Desc = "–ü—Ä–µ–¥–º–µ—Ç '" .. tool.Name .. "' –¥—é–ø–Ω—É—Ç!",
                    Time = 3
                })
            end)
            
            if not success then
                Window:Notify({
                    Title = "‚ùå –û—à–∏–±–∫–∞",
                    Desc = "–û—à–∏–±–∫–∞ –ø—Ä–∏ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–∏",
                    Time = 3
                })
            end
        else
            Window:Notify({
                Title = "‚ö†Ô∏è –í–Ω–∏–º–∞–Ω–∏–µ",
                Desc = "–í–æ–∑—å–º–∏ –ø—Ä–µ–¥–º–µ—Ç –≤ —Ä—É–∫–∏!",
                Time = 3
            })
        end
    end
})

-- –¢–æ–≥–≥–ª –¥–ª—è –≥–æ—Ä—è—á–µ–π –∫–ª–∞–≤–∏—à–∏
