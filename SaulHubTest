    Window:Notify({
        Title = "üéÆ –û–±–Ω–∞—Ä—É–∂–µ–Ω–∞ –∏–≥—Ä–∞: " .. currentGameData.name,
        Desc = "–ê–≤—Ç–æ-—Å–∫—Ä–∏–ø—Ç—ã –¥–æ—Å—Ç—É–ø–Ω—ã! üéØ",
        Time = 7
    })
