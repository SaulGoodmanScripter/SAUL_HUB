local antiVoidEnabled = false
local voidYLevel = -10
local teleportPosition = Vector3.new(0, 50, 0)
local checkInterval = 0.1

Tab2:Toggle({
    Title = "Anti-Void",
    Desc = "Protected from falling into the abyss",
    Value = false,
    Callback = function(v)
        antiVoidEnabled = v
        togglesound()
    end
})

Tab2:Slider({
    Title = "Void Level",
    Desc = "Y coordinate below which to teleport",
    Min = -100,
    Max = 50,
    Rounding = 0,
    Value = -10,
    Callback = function(val)
        voidYLevel = val
        slidersound()
    end
})

Tab2:Button({
    Title = "Set Safe Position",
    Desc = "Set current position as safe",
    Callback = function()
        btnclick()
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            teleportPosition = char.HumanoidRootPart.Position + Vector3.new(0, 5, 0)
            Window:Notify({
                Title = "Position Saved",
                Desc = string.format("X:%.1f Y:%.1f Z:%.1f", 
                    teleportPosition.X, teleportPosition.Y, teleportPosition.Z),
                Time = 3
            })
        end
    end
})

-- Anti-Void function
local antiVoidConnection
spawn(function()
    while wait(checkInterval) do
        if antiVoidEnabled and LocalPlayer.Character then
            local char = LocalPlayer.Character
            local rootPart = char:FindFirstChild("HumanoidRootPart")

            if rootPart and rootPart.Position.Y < voidYLevel then
                -- Teleport to safe position
                rootPart.CFrame = CFrame.new(teleportPosition)

                -- Visual effect
                Window:Notify({
                    Title = "Anti-Void Activated!",
                    Desc = string.format("Teleported from Y:%.1f", rootPart.Position.Y),
                    Time = 3
                })

                -- Sound effect
                pcall(function()
                    local sound = Instance.new("Sound")
                    sound.SoundId = "rbxassetid://911882909"
                    sound.Volume = 0.5
                    sound.Parent = workspace
                    sound:Play()
                    wait(1)
                    sound:Destroy()
                end)
            end
        end
    end
end)

