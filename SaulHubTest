    Tab6:Button({
        Title = exec.icon .. " " .. exec.name,
        Desc = exec.desc,
        Callback = function()
            btnclick()
            
            Window:Notify({
                Title = exec.icon .. " –ó–∞–≥—Ä—É–∑–∫–∞ " .. exec.name,
                Desc = "–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –ø–æ–¥–æ–∂–¥–∏—Ç–µ...",
                Time = 3
            })
            
            -- –ó–∞–≥—Ä—É–∂–∞–µ–º —Å–∫—Ä–∏–ø—Ç —ç–∫—Å–ø–ª–æ–∏—Ç–∞
            local success, result = pcall(function()
                local scriptContent = game:HttpGet(exec.url, true)
                if scriptContent then
                    local fn = loadstring(scriptContent)
                    if fn then
                        fn()
                        Window:Notify({
                            Title = "‚úÖ " .. exec.name .. " –ó–∞–≥—Ä—É–∂–µ–Ω",
                            Desc = "–ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å —ç–∫—Å–ø–ª–æ–∏—Ç–∞ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!",
                            Time = 4
                        })
                        notifysound()
                    else
                        error("–û—à–∏–±–∫–∞ –∫–æ–º–ø–∏–ª—è—Ü–∏–∏")
                    end
                else
                    error("–ù–µ —É–¥–∞–ª–æ—Å—å –∑–∞–≥—Ä—É–∑–∏—Ç—å —Å–∫—Ä–∏–ø—Ç")
                end
            end)
            
            if not success then
                Window:Notify({
                    Title = "‚ùå –û—à–∏–±–∫–∞",
                    Desc = "–ù–µ —É–¥–∞–ª–æ—Å—å –∑–∞–≥—Ä—É–∑–∏—Ç—å " .. exec.name,
                    Time = 5
                })
            end
        end
    })
end

Tab6:Section({Title = "üìù Custom Script Executor"})

local defaultScript = [[-- üéÑ Saul Hub Executor
print("Hello from Saul Hub!")

-- –ü—Ä–∏–º–µ—Ä —Å–∫—Ä–∏–ø—Ç–∞
local player = game.Players.LocalPlayer

if player.Character then
    local humanoid = player.Character:FindFirstChild("Humanoid")
    if humanoid then
        humanoid.WalkSpeed = 50
        humanoid.JumpPower = 100
        print("–°–∫–æ—Ä–æ—Å—Ç—å —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞!")
    end
end]]

local currentScript = defaultScript

local codeDisplay = Tab6:Code({
    Title = "üìù –¢–µ–∫—É—â–∏–π —Å–∫—Ä–∏–ø—Ç",
    Code = currentScript
})

local scriptInfo = Tab6:Label({
    Title = "üìä –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è",
    Desc = "–°–∏–º–≤–æ–ª–æ–≤: " .. #currentScript .. " | –°—Ç—Ä–æ–∫: " .. select(2, string.gsub(currentScript, "\n", "")) + 1
})

local function updateScriptInfo()
    local charCount = #currentScript
    local lineCount = select(2, string.gsub(currentScript, "\n", "")) + 1
    scriptInfo:SetDesc(string.format("–°–∏–º–≤–æ–ª–æ–≤: %d | –°—Ç—Ä–æ–∫: %d", charCount, lineCount))
end

local scriptInput = Tab6:Textbox({
    Title = "‚úèÔ∏è –†–µ–¥–∞–∫—Ç–∏—Ä–æ–≤–∞—Ç—å —Å–∫—Ä–∏–ø—Ç",
    Desc = "–í–≤–µ–¥–∏—Ç–µ –≤–∞—à Lua –∫–æ–¥",
    Placeholder = "print('–ü—Ä–∏–≤–µ—Ç –æ—Ç Saul Hub!')",
    Value = "",
    ClearText = false,
    Callback = function(text)
        if text and text ~= "" then
            currentScript = text
            codeDisplay:SetCode(text)
            updateScriptInfo()
        end
    end
})

Tab6:Section({Title = "‚öôÔ∏è –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ —Å–∫—Ä–∏–ø—Ç–∞–º–∏"})

Tab6:Button({
    Title = "üöÄ –í—ã–ø–æ–ª–Ω–∏—Ç—å",
    Desc = "–ó–∞–ø—É—Å—Ç–∏—Ç—å —Ç–µ–∫—É—â–∏–π —Å–∫—Ä–∏–ø—Ç",
    Callback = function()
        btnclick()

        if not currentScript or currentScript:gsub("%s", "") == "" then
            Window:Notify({
                Title = "‚ö†Ô∏è –í–Ω–∏–º–∞–Ω–∏–µ",
                Desc = "–°–∫—Ä–∏–ø—Ç –ø—É—Å—Ç–æ–π!",
                Time = 3
            })
            return
        end

        Window:Notify({
            Title = "‚ö° –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ...",
            Desc = "–°–∫—Ä–∏–ø—Ç –∑–∞–ø—É—Å–∫–∞–µ—Ç—Å—è",
            Time = 2
        })

        local success, result = pcall(function()
            local fn = loadstring(currentScript)
            if fn then
                return fn()
            else
                error("–û—à–∏–±–∫–∞ –∫–æ–º–ø–∏–ª—è—Ü–∏–∏")
            end
        end)

        if success then
            Window:Notify({
                Title = "‚úÖ –£—Å–ø–µ—à–Ω–æ",
                Desc = "–°–∫—Ä–∏–ø—Ç –≤—ã–ø–æ–ª–Ω–µ–Ω",
                Time = 3
            })
        else
            local errorMsg = tostring(result):sub(1, 100)
            Window:Notify({
                Title = "‚ùå –û—à–∏–±–∫–∞",
                Desc = "–û—à–∏–±–∫–∞: " .. errorMsg,
                Time = 5
            })
        end
    end
})

Tab6:Button({
    Title = "üìã –í—Å—Ç–∞–≤–∏—Ç—å –∏–∑ –±—É—Ñ–µ—Ä–∞",
    Desc = "–í—Å—Ç–∞–≤–∏—Ç—å —Å–∫—Ä–∏–ø—Ç –∏–∑ –±—É—Ñ–µ—Ä–∞ –æ–±–º–µ–Ω–∞",
    Callback = function()
        btnclick()
        pcall(function()
            local clipboard = game:HttpGet("http://127.0.0.1:6463/rpc?v=1", true)
            if clipboard then
                currentScript = clipboard
                codeDisplay:SetCode(clipboard)
                updateScriptInfo()
                Window:Notify({
                    Title = "üìã –í—Å—Ç–∞–≤–ª–µ–Ω–æ",
                    Desc = "–°–∫—Ä–∏–ø—Ç –∑–∞–≥—Ä—É–∂–µ–Ω –∏–∑ –±—É—Ñ–µ—Ä–∞",
                    Time = 3
                })
            end
        end)
    end
})

