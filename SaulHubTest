})

if currentGameData then
    Window:Notify({
        Title = "Game Detected: " .. currentGameData.name,
        Desc = "Auto-scripts available!",
        Time = 7
    })
else
    Window:Notify({
        Title = "Game not found in database",
        Desc = "ID: " .. game.PlaceId .. "\nUse universal scripts",
        Time = 5
    })
end