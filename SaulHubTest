Tab4:Button({
    Title = "üéÆ –¢–µ—Å—Ç –≤–∏—Ä—Ç—É–∞–ª—å–Ω–æ–≥–æ –≤–≤–æ–¥–∞",
    Desc = "–ü—Ä–æ—Ç–µ—Å—Ç–∏—Ä–æ–≤–∞—Ç—å –Ω–∞ —ç–∫—Ä–∞–Ω–µ",
    Callback = function()
        btnclick()
        simulateVirtualClick()
        Window:Notify({
            Title = "üì± –¢–µ—Å—Ç",
            Desc = "–í–∏—Ä—Ç—É–∞–ª—å–Ω—ã–π –∫–ª–∏–∫ –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω",
            Time = 2
        })
    end
})

-- –§—É–Ω–∫—Ü–∏—è –≤–∏—Ä—Ç—É–∞–ª—å–Ω–æ–≥–æ –∫–ª–∏–∫–∞
local function simulateVirtualClick()
    local VirtualInputManager = game:GetService("VirtualInputManager")
    local GuiService = game:GetService("GuiService")
    
    -- –ü–æ–ª—É—á–∞–µ–º —Ä–∞–∑–º–µ—Ä —ç–∫—Ä–∞–Ω–∞
    local viewportSize = workspace.CurrentCamera.ViewportSize
    local centerPos = Vector2.new(viewportSize.X / 2, viewportSize.Y / 2)
    
    if clickType == "–¢–∞–ø" then
        -- –í–∏—Ä—Ç—É–∞–ª—å–Ω—ã–π —Ç–∞–ø (–Ω–∞–∂–∞—Ç–∏–µ –∏ –æ—Ç–ø—É—Å–∫–∞–Ω–∏–µ)
        VirtualInputManager:SendMouseButtonEvent(
            centerPos.X, centerPos.Y, -- –ø–æ–∑–∏—Ü–∏—è
            0, -- –∫–Ω–æ–ø–∫–∞ (0 = –ª–µ–≤–∞—è)
            true, -- –Ω–∞–∂–∞—Ç–∏–µ
            game, -- game
            1 -- ?
        )
        
        wait(0.05)
        
        VirtualInputManager:SendMouseButtonEvent(
            centerPos.X, centerPos.Y,
            0,
            false, -- –æ—Ç–ø—É—Å–∫–∞–Ω–∏–µ
            game,
            1
        )
        
    elseif clickType == "–£–¥–µ—Ä–∂–∞–Ω–∏–µ" then
        -- –î–æ–ª–≥–æ–µ –Ω–∞–∂–∞—Ç–∏–µ
        VirtualInputManager:SendMouseButtonEvent(
            centerPos.X, centerPos.Y,
            0,
            true,
            game,
            1
        )
        
        wait(0.5) -- —É–¥–µ—Ä–∂–∏–≤–∞–µ–º 0.5 —Å–µ–∫—É–Ω–¥
        
        VirtualInputManager:SendMouseButtonEvent(
            centerPos.X, centerPos.Y,
            0,
            false,
            game,
            1
        )
        
    elseif clickType == "–°–≤–∞–π–ø" then
        -- –°–≤–∞–π–ø (–ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏–µ)
        local startPos = Vector2.new(centerPos.X - 50, centerPos.Y)
        local endPos = Vector2.new(centerPos.X + 50, centerPos.Y)
        
        -- –ù–∞—á–∞–ª–æ –∫–∞—Å–∞–Ω–∏—è
        VirtualInputManager:SendMouseButtonEvent(
            startPos.X, startPos.Y,
            0,
            true,
            game,
            1
        )
        
        -- –î–≤–∏–≥–∞–µ–º –ø–∞–ª–µ—Ü
        for i = 1, 10 do
            local pos = startPos:Lerp(endPos, i/10)
            VirtualInputManager:SendMouseMoveEvent(
                pos.X, pos.Y,
                game
            )
            wait(0.02)
        end
        
        -- –û—Ç–ø—É—Å–∫–∞–µ–º
        VirtualInputManager:SendMouseButtonEvent(
            endPos.X, endPos.Y,
            0,
            false,
            game,
            1
        )
    end
end

-- –ê–≤—Ç–æ–∫–ª–∏–∫–µ—Ä
spawn(function()
    while wait() do
        if virtualClickEnabled then
            simulateVirtualClick()
            wait(clickInterval)
        end
    end
end)
