})

Tab8:Label({
    Title = "Statistics",
    Desc = "Players in server: " .. #Players:GetPlayers() .. "/" .. Players.MaxPlayers .. 
           "\nGame ID: " .. game.PlaceId
})

-- Кнопка поддержки
Tab8:Section({Title = "Support"})

Tab8:Button({
    Title = "Report Issue",
    Desc = "Report bug or suggest feature",
    Callback = function()
        btnclick()
        local issueMessage = [[Saul Hub Issue Report:
        
Game: PlaceID ]] .. game.PlaceId .. [[
Time: ]] .. os.date("%Y-%m-%d %H:%M:%S") .. [[
        
Issue Description:
]]
        
        setclipboard(issueMessage)
        Window:Notify({
            Title = "Report Template",
            Desc = "Template copied to clipboard\nDescribe issue and send to developer",
            Time = 5
        })
    end
})

Tab8:Button({
    Title = "Check for Updates",
    Desc = "Get latest version",
    Callback = function()
        btnclick()
        Window:Notify({
            Title = "Updates",
            Desc = "Current: v4.0\nCheck GitHub for new releases",
            Time = 4
        })
        
        setclipboard("https://github.com/SaulGoodmanScripter/SaulHub")
    end
})

-- Информация о библиотеке
Tab8:Section({Title = "Technical"})

