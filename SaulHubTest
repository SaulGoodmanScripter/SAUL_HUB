-- LOW POP SERVERS TAB
local Tab7 = Window:Tab({Title = "Low Pop", Icon = "users"})

Tab7:Section({Title = "Search Low Population Servers"})

local serverList = {}
local isLoading = false
local maxServers = 20

local statusLabel = Tab7:Label({
    Title = "Ready to search",
    Desc = "Press 'Find Servers' to start"
})

local function fetchServers(sortOrder)
    local success, result = pcall(function()
        local url = string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=%s&limit=100", 
            game.PlaceId, sortOrder or "Asc")

        local response = game:HttpGet(url, true)
        return game:GetService("HttpService"):JSONDecode(response)
    end)

    return success and result or nil
end

local function createServerButtons()
    if #serverList == 0 then
        Tab7:Label({
            Title = "No data",
            Desc = "Press 'Find Servers'"
        })
        return
    end

    local displayed = 0
    for _, server in ipairs(serverList) do
        if displayed >= maxServers then break end

        local playerCount = server.playing
        local maxPlayers = server.maxPlayers
        local fillPercentage = math.floor((playerCount / maxPlayers) * 100)

        local status = ""
        local statusColor = ""
        if playerCount == 0 then
            status = "EMPTY"
        elseif playerCount <= 3 then
            status = "LOW"
        elseif playerCount <= 10 then
            status = "MEDIUM"
        else
            status = "HIGH"
        end

        Tab7:Button({
            Title = string.format("%d/%d | %s", playerCount, maxPlayers, status),
            Desc = string.format("Fill: %d%% | ID: %s", fillPercentage, server.id or "Unknown"),
            Callback = function()
                btnclick()

                if server.id then
                    Window:Notify({
                        Title = "Teleporting...",
                        Desc = string.format("To server with %d players", playerCount),
                        Time = 3
                    })

                    pcall(function()
                        game:GetService("TeleportService"):TeleportToPlaceInstance(
                            game.PlaceId,
                            server.id,
                            LocalPlayer
                        )
                    end)
                else
                    Window:Notify({
                        Title = "Error",
                        Desc = "Server ID not found",
                        Time = 3
                    })
                end
            end
        })

        displayed = displayed + 1
    end
end

Tab7:Section({Title = "Controls"})

Tab7:Button({
    Title = "Find Servers (Less Players)",
    Desc = "Sort from empty to full",
    Callback = function()
        btnclick()

        if isLoading then return end
        isLoading = true

        statusLabel:SetTitle("Searching...")
        statusLabel:SetDesc("Requesting server list")

        spawn(function()
            local data = fetchServers("Asc")

            if data and data.data then
                serverList = data.data

                local filtered = {}
                for _, server in ipairs(serverList) do
                    if server.playing < server.maxPlayers then
                        table.insert(filtered, server)
                    end
                end
                serverList = filtered

                statusLabel:SetTitle(string.format("Found: %d", #serverList))
                statusLabel:SetDesc(string.format("Servers with < %d players", maxServers))

                createServerButtons()

                Window:Notify({
                    Title = "Servers Found",
                    Desc = string.format("Found %d available servers", #serverList),
                    Time = 3
                })
            else
                statusLabel:SetTitle("Error")
                statusLabel:SetDesc("Failed to get server list")

                Window:Notify({
                    Title = "Error",
                    Desc = "Check internet connection",
                    Time = 5
                })
            end

            isLoading = false
        end)
    end
})

Tab7:Button({
    Title = "Update List",
    Desc = "Update current server list",
    Callback = function()
        btnclick()
        if #serverList > 0 then
            createServerButtons()
            Window:Notify({
                Title = "Updated",
                Desc = "Server list updated",
                Time = 2
            })
        end
    end
})

Tab7:Button({
    Title = "Fast Rejoin",
    Desc = "Instantly rejoin game",
    Callback = function()
        btnclick()
        game:GetService("TeleportService"):Teleport(game.PlaceId, LocalPlayer)
    end
})

Tab7:Section({Title = "Settings"})

Tab7:Slider({
    Title = "Max Servers",
    Desc = "How many servers to show",
    Min = 5,
    Max = 50,
    Rounding = 0,
    Value = 20,
    Callback = function(val)
        maxServers = val
        slidersound()

        if #serverList > 0 then
            createServerButtons()
        end
    end
})

Tab7:Toggle({
    Title = "Auto Search",
    Desc = "Automatically search servers when opened",
    Value = false,
    Callback = function(v)
        autoSearch = v
        togglesound()

        if v and #serverList == 0 then
            local searchBtn = Tab7.Buttons["Find Servers (Less Players)"]
            if searchBtn then
                searchBtn.Callback()
            end
        end
    end
})

Tab7:Section({Title = "Information"})

local gameName = "Unknown Game"
local gameId = game.PlaceId

pcall(function()
    local info = game:GetService("MarketplaceService"):GetProductInfo(gameId)
    gameName = info.Name or gameName
end)

Tab7:Label({
    Title = "Current Game:",
    Desc = gameName .. "\nID: " .. gameId
})

Tab7:Label({
    Title = "Statistics:",
    Desc = string.format("Players online: %d/%d", 
        #game:GetService("Players"):GetPlayers(),
        game:GetService("Players").MaxPlayers)
})

Tab7:Opened(function()
    if #serverList == 0 then
        Tab7:Label({
            Title = "Instructions:",
            Desc = "1. Press 'Find Servers'\n2. Select server from list\n3. Click to teleport"
        })
    end
end)

spawn(function()
    wait(2)
    if autoSearch and #serverList == 0 then
        local searchBtn = Tab7.Buttons["Find Servers (Less Players)"]
        if searchBtn then
            searchBtn.Callback()
        end
    end
end)

