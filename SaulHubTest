local function loadScriptSafe(url)
    Window:Notify({
        Title = "Loading...",
        Desc = "Please wait",
        Time = 2
    })

    local success, result = pcall(function()
        local scriptContent = game:HttpGet(url, true)
        if scriptContent and scriptContent ~= "404: Not Found" then
            local loadedFunction = loadstring(scriptContent)
            if loadedFunction then
                loadedFunction()
                Window:Notify({
                    Title = "Success",
                    Desc = "Script loaded successfully!",
                    Time = 3
                })
            else
                error("Script compilation error")
            end
        else
            error("Script not found")
        end
    end)

    if not success then
        Window:Notify({
            Title = "Error",
            Desc = "Failed to load script",
            Time = 5
        })
    end
end

Tab6:Section({Title = "Auto-Search Current Game"})

local currentGameId = game.PlaceId
local currentGameData = nil

for _, gameData in pairs(GameDatabase) do
    for _, placeId in ipairs(gameData.placeIds) do
        if placeId == currentGameId then
            currentGameData = gameData
            break
        end
    end
    if currentGameData then break end
end

if currentGameData then
    Tab6:Label({
        Title = "Game Found!",
        Desc = currentGameData.name
    })

    for _, script in ipairs(currentGameData.scripts) do
        Tab6:Code({
            Title = script.title,
            Code = "loadstring(game:HttpGet('" .. script.url .. "'))()"
        })

        Tab6:Button({
            Title = "Load " .. script.title,
            Desc = "Auto load script",
            Callback = function()
                loadScriptSafe(script.url)
            end
        })
    end
else
    local gameName = "Unknown Game"
    pcall(function()
        gameName = MarketplaceService:GetProductInfo(currentGameId).Name
    end)

    Tab6:Label({
        Title = "Game Not Found",
        Desc = gameName .. "\nID: " .. currentGameId .. "\nCheck games below"
    })
end

Tab6:Section({Title = "All Games & Scripts"})

for gameKey, gameData in pairs(GameDatabase) do
    Tab6:Label({
        Title = gameData.name,
        Desc = "Game ID(s): " .. table.concat(gameData.placeIds, ", ")
    })

    for _, script in ipairs(gameData.scripts) do
        Tab6:Code({
            Title = script.title,
            Code = "loadstring(game:HttpGet('" .. script.url .. "'))()"
        })

        Tab6:Button({
            Title = "Load " .. script.title,
            Desc = "Load " .. script.title,
            Callback = function()
                loadScriptSafe(script.url)
            end
        })
    end

    Tab6:Label({
        Title = "─",
        Desc = ""
    })
end

